                    },
                    {
                        "name": "y",
                        "schema": "double"
                    },
                    {
                        "name": "z",
                        "schema": "double"
                    }
                ]
            }
        },
        {
            "@type": "Command",
            "description": "This command will display a message on the Sense Hat LED display.",
            "name": "show_message",
            "commandType": "synchronous",
            "request": {
              "name": "message",
              "schema": "string"
            }
        }
	]
